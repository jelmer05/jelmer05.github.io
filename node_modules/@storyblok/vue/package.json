{
  "name": "@storyblok/vue",
  "type": "module",
  "version": "9.3.3",
  "description": "SDK to integrate Storyblok into your project using Vue.",
  "author": "Storyblok",
  "license": "MIT",
  "homepage": "https://github.com/storyblok/monoblok/tree/main/packages/vue#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/monoblok.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/storyblok/monoblok/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/storyblok-vue.mjs",
      "require": "./dist/storyblok-vue.js"
    }
  },
  "main": "./dist/storyblok-vue.js",
  "module": "./dist/storyblok-vue.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": ">=3.4"
  },
  "dependencies": {
    "@storyblok/js": "4.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.15.18",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "cypress": "^14.3.3",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "^9.32.0",
    "kolorist": "^1.8.0",
    "pathe": "^2.0.3",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-banner": "^0.8.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.0",
    "@storyblok/eslint-config": "0.4.1"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "implicitDependencies": [
      "!@storyblok/playground-*"
    ]
  },
  "scripts": {
    "dev": "vite build --watch",
    "playground": "pnpm run --filter ./playground/vue dev",
    "test": "pnpm run test:unit:ci && pnpm run test:e2e",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "pnpm run cy:components",
    "build": "vite build",
    "test:types": "tsc --noEmit --skipLibCheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:components": "cypress run --component"
  }
}